{
	"name": "go-web",
	"displayName": "Go-Web",
	"publisher": "Roberto",
	"description": "Go-Web Framework helper.",
	"icon": "img/logo.png",
	"version": "1.0.5",
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/RobyFerro/go-web-vscode-ext.git"
	},
	"activationEvents": [
		"onCommand:goweb.install",
		"onCommand:goweb.createController",
		"onCommand:goweb.createMiddleware",
		"onCommand:goweb.createMigration",
		"onCommand:goweb.createCommand",
		"onCommand:goweb.createModel",
		"onCommand:goweb.generateKey",
		"onCommand:goweb.createJob"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "goweb.install",
				"title": "Go-Web: New Project"
			},
			{
				"command": "goweb.createController",
				"title": "Go-Web: Create controller"
			},
			{
				"command": "goweb.createMiddleware",
				"title": "Go-Web: Create middleware"
			},
			{
				"command": "goweb.createMigration",
				"title": "Go-Web: Create migration"
			},
			{
				"command": "goweb.createCommand",
				"title": "Go-Web: Create command"
			},
			{
				"command": "goweb.createModel",
				"title": "Go-Web: Create model"
			},
			{
				"command": "goweb.generateKey",
				"title": "Go-Web: Generate app key"
			},
			{
				"command": "goweb.createJob",
				"title": "Go-Web: Generate async job"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.12.28",
		"@types/vscode": "^1.42.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"@typescript-eslint/parser": "^2.18.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"@types/shelljs": "^0.8.6",
		"shelljs": "^0.8.3",
		"simple-git": "^1.131.0"
	}
}
